{
  "manifest_version": 2,

  "name": "LocalAssets",
  "description": "Replaces local asset src urls with production ones.",
  "version": "1.0",

  "options_page": "options.html",

  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jQuery.min.js", "content_script.js"]
    }
  ]
}
